<!DOCTYPE html>
<link rel="STYLESHEET" href="/css/styleProfiel.css" type="text/css">
<h1>Matches:</h1>
<!-- Maakt een div aan met flashmessages met class error of succes -->
<%- messages() %>
    <!-- <% if (data.length > 0){ %> -->
    <% for (var i = 0; i < data.length; i++){ %>
        <ul class="matches">
            <h2>
                <%= data[i].voornaam %>
            </h2>
            <p>
                <%= data[i].achternaam %>
            </p>
            <img src="images/profielfotos/<%= data[i].photo %>" alt="Profilepicture">
        </ul>

        <% } %>
            <% }else { %>
                <p>U heeft nog geen matches</p>
                <% } %>